<h1>Current Categories Index</h1>

<h4><%= link_to "Create New Category", new_category_path %>

<ul>
<% @categories.each do |category| %>
  <h3><%= link_to category.title, category_path(category)%></h3>
  <li><%= link_to "edit", edit_category_path(category) %></li>
  <li class="delete"><%= link_to "delete", category_path(category), method: :delete %></li>
<% end %>
</ul>
